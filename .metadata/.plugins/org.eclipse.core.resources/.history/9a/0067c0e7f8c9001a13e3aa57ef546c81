
public class TicTacToe {

	public static void main(String[] args) {
		
		//Creating Game board 
		char[][] gameBoard = {{' ', '|', ' ', '|', ' '},
				{'-', '+', '-', '+', '-'},
				{' ', '|', ' ', '|', ' '},
				{'-', '+', '-', '+', '-'},
				{' ', '|', ' ', '|', ' '}};

		//Printing out the gameboard using a for loop
		printGameBoard(gameBoard);
	}
	public static void printGameBoard(char[] [] gameBoard) {
		for(char[] row: gameBoard) {
			for(char c: row) {
				System.out.print(c);
			}
			System.out.println();
		}
	}
}
